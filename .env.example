# Environment
ENV=development
DEBUG=true

# API Settings
OPENAI_API_KEY=your_openai_api_key_here
MODEL_NAME=gpt-4-1106-preview
TEMPERATURE=0.7

# Business Settings
BUSINESS_NAME=Your Business Name
BUSINESS_TYPE=restaurant  # or retail
BUSINESS_DESCRIPTION=Your business description
LOCATION=Calhoun, GA

# Database Settings
DATABASE_URL=sqlite:///./business.db
PROD_DATABASE_URL=postgresql://user:password@localhost:5432/business

# Redis Settings
REDIS_URL=redis://localhost:6379

# Security Settings
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Logging Settings
LOG_LEVEL=INFO
LOG_FILE=business_automation.log

# Agent Settings
AGENT_MEMORY_TTL=3600
MAX_RETRIES=3
TIMEOUT=300

# Local Business Integration
LOCAL_SUPPLIERS_FILE=local_suppliers.json
COMMUNITY_EVENTS_FILE=community_events.json

# Feature Flags
ENABLE_HIPAA=true
ENABLE_AUDIT_LOGGING=true
ENABLE_CACHING=true

# Groq API Key
VITE_GROQ_API_KEY=your_groq_api_key_here 